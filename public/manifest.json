{
    "manifest_version": 3,
    "name": "Url tools",
    "version": "1.0.0",
    "description": "A Chrome extension to parse and manipulate URLs.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "clipboardRead",
        "clipboardWrite"
    ]
}
